{
  "name": "@cloudoperators/juno-url-state-provider",
  "version": "3.0.4",
  "description": "Save several paths in the url. This makes it possible to manage the status of several apps such as micro frontends with the help of URL.",
  "author": "UI-Team",
  "contributors": [
    "Tilman Haupt",
    "Andreas Pfau"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudoperators/juno.git#main"
  },
  "source": "src/index.ts",
  "main": "build/index.cjs.js",
  "module": "build/index.es.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.es.js",
      "require": "./build/index.cjs.js"
    }
  },
  "files": [
    "build"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf build && rm -rf node_modules && rm -rf .turbo",
    "clean:cache": "rm -rf .turbo"
  },
  "devDependencies": {
    "@cloudoperators/juno-config": "workspace:*",
    "jsdom": "^26.1.0",
    "juri-cutlery": "^1.0.0",
    "lz-string": "^1.4.4",
    "typescript": "^5.8.3",
    "vite": "^7.0.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "history": "^5.3.0",
    "juri": "^1.0.3",
    "query-string": "9.1.1"
  }
}
