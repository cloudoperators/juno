{
  "name": "@cloudoperators/juno-communicator",
  "version": "2.2.7",
  "description": "Send and receive across tabs",
  "author": "UI-Team",
  "contributors": [
    "Andreas Pfau"
  ],
  "repository": "https://github.com/cloudoperators/juno/tree/main/packages/communicator",
  "license": "Apache-2.0",
  "source": "src/index.js",
  "main": "build/index.js",
  "module": "build/index.js",
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=10.0.0 <11.0.0"
  },
  "scripts": {
    "devOld": "NODE_ENV=development node ./esbuild.config.js --watch",
    "build": "NODE_ENV=production node ./esbuild.config.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.4.2",
    "esbuild": "^0.17.6",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.7.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "transform": {
      "\\.js$": "babel-jest"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/dev/",
      "<rootDir>/build/"
    ]
