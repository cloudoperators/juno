      },
      "status": "active",
      "title": "Test Template - NonExistingTestNamespaceProbesFailed"
    }
  ],
  "predefinedFilters": [
    {
      "name": "all",
      "displayName": "All",
      "matchers": {
        "region": ".*"
      }
    }
  ]
